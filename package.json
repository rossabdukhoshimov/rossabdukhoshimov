{
  "name": "@playwright-cucumber-framework/root",
  "description": "Test Automation with Playwright",
  "version": "1.0.0",
  "scripts": {
    "test": "cucumber-js test",
    "login": "cucumber-js test -p runner --tags @login_user",
    "parallel:test": "cucumber-js test -p runner --tags 'not @sequential' --tags",
    "sequential:test": "cucumber-js test -p no_thread --tags '@sequential' --tags",
    "rerun": "cucumber-js -p rerun"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.1.2",
    "@playwright/test": "^1.44.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.11",
    "dotenv": "^16.4.5",
    "fetch-to-curl": "^0.6.0",
    "fs-extra": "^11.2.0",
    "randomstring": "^1.3.0",
    "string-format": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "winston": "^3.13.0"
  },
  "dependencies": {
    "@aws-sdk/client-glue": "^3.621.0",
    "@aws-sdk/client-s3": "^3.617.0",
    "@aws-sdk/client-ssm": "^3.592.0",
    "@smithy/node-http-handler": "^2.2.2",
    "authenticator": "^1.1.5",
    "cucumber-html-reporter": "^6.0.0",
    "jsonpath": "^1.1.1",
    "moment": "^2.30.1",
    "mssql": "^7.2.0",
    "oracledb": "^6.5.1",
    "otpauth": "^9.3.1",
    "semver": "^7.5.2",
    "xpath": "^0.0.34"
  }
}
